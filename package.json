{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler d1 migrations apply mydb && remix build && wrangler pages dev ./public",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development pnpm run wrangler:page",
    "dev:studio": "DATABASE_URL=file:../.wrangler/state/d1/DB.sqlite3 prisma studio",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production pnpm run wrangler",
    "typecheck": "tsc -b",
    "migration": "zx ./scripts/migration.mjs",
    "wrangler:page": "wrangler pages dev ./public --local --persist --port 3000"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.10.0",
    "@remix-run/cloudflare-pages": "^1.10.0",
    "@remix-run/react": "^1.10.0",
    "better-sqlite3": "^8.0.1",
    "cross-env": "^7.0.3",
    "kysely": "^0.23.3",
    "kysely-d1": "^0.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@prisma/client": "^4.8.1",
    "@remix-run/dev": "^1.10.0",
    "@remix-run/eslint-config": "^1.10.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "kysely-codegen": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.8.1",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1",
    "zx": "^7.1.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
